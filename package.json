{
  "name": "sveltekit-oidc",
  "version": "0.0.8",
  "scripts": {
    "dev": "DEBUG=sveltekit-oidc:* VITE_OIDC_CLIENT_SECRET=$(kubectl get secret -n auth keycloak-client-secret-local-public -o 'jsonpath={.data.CLIENT_SECRET}' | base64 -D) svelte-kit dev",
    "build": "VITE_OIDC_CLIENT_SECRET=$(kubectl get secret -n auth keycloak-client-secret-local-public -o 'jsonpath={.data.CLIENT_SECRET}' | base64 -D) svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.328",
    "@types/cookie": "0.5.1",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@urql/svelte": "1.3.3",
    "autoprefixer": "10.4.7",
    "cross-fetch": "3.1.5",
    "cssnano": "5.1.7",
    "debug": "4.3.4",
    "dotenv-cli": "5.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "graphql-ws": "5.8.2",
    "jwt-decode": "3.1.2",
    "postcss": "8.4.13",
    "postcss-import": "14.1.0",
    "postcss-load-config": "3.1.4",
    "postcss-preset-env": "7.5.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "semantic-release": "19.0.2",
    "svelte": "3.48.0",
    "svelte-check": "2.7.0",
    "svelte-preprocess": "4.10.6",
    "svelte2tsx": "0.5.9",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "ws": "8.6.0"
  },
  "peerDependencies": {
    "@urql/svelte": "1.x",
    "debug": "4.x",
    "graphql-ws": "5.x",
    "jwt-decode": "3.x",
    "ws": "8.x"
  },
  "license": "MIT",
  "keywords": [
    "sveltekit-oidc",
    "auth",
    "authentication",
    "jwt",
    "nodejs",
    "oauth2",
    "oidc",
    "sveltekit"
  ],
  "author": "Tushar Shukla",
  "contributors": [
    {
      "email": "pat@patscott.io",
      "name": "Patrick Lee Scott"
    }
  ],
  "repository": "https://github.com/tushar10sh/sveltekit-oidc.git",
  "type": "module"
}
